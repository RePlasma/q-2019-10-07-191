 RowBox[{
  RowBox[{"\[Theta]0", "=", 
   RowBox[{"{", 
    RowBox[{"0.1734", ",", "0.3909"}], "}"}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"rotations", " ", "on", " ", "wavefunction"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Rx", "=", 
   RowBox[{"MatrixExp", "[", 
    RowBox[{
     RowBox[{"-", "I"}], " ", "\[Theta]x", " ", 
