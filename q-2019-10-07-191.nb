      RowBox[{"{", 
       RowBox[{"Axx", ",", "Axz"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"Azx", ",", "Azz"}], "}"}]}], "}"}], "//", "FullSimplify"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Ci", "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Cx", ",", "Cz"}], "}"}], "//", "FullSimplify"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "take", " ", "real", " ", "and", " ", "imag", " ", "parts", " ", "of", " ",
     "A", " ", "and", " ", "C", " ", "respectively"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"AR", "=", 
   RowBox[{"Refine", "[", 
    RowBox[{
     RowBox[{"Re", "[", "A", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[Theta]x", ">", "0"}], ",", 
