Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "X", ",", "Y", ",", "Z", ",", "Rx", ",", "Rz", ",", "\[Theta]x", ",", 
    "\[Theta]z", ",", "H", ",", "t", ",", "A", ",", "Ci", ",", "Cx", ",", 
    "Cz", ",", "d\[Theta]", ",", "s"}], "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Pauli", " ", "terms"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{"X", ",", "Y", ",", "Z"}], "}"}], "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"PauliMatrix", "[", "i", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "3"}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"initial", " ", "conditions"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Theta]0", "=", 
   RowBox[{"{", 
    RowBox[{"0.1734", ",", "0.3909"}], "}"}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"rotations", " ", "on", " ", "wavefunction"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Rx", "=", 
   RowBox[{"MatrixExp", "[", 
    RowBox[{
     RowBox[{"-", "I"}], " ", "\[Theta]x", " ", 
     RowBox[{"X", "/", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Rz", "=", 
    RowBox[{"MatrixExp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], " ", "\[Theta]z", " ", 
      RowBox[{"Z", "/", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"parametrized", " ", "wavefunction"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Psi]", "=", 
   RowBox[{"Rz", ".", "Rx", ".", 
    RowBox[{"{", 
     RowBox[{"1", ",", "0"}], "}"}]}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", "Hamiltonian", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"H", "=", "Y"}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"derivatives", " ", "of", " ", "wavefunction"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dx\[Psi]", "=", 
   RowBox[{"D", "[", 
    RowBox[{"\[Psi]", ",", "\[Theta]x"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"dz\[Psi]", "=", 
    RowBox[{"D", "[", 
     RowBox[{"\[Psi]", ",", "\[Theta]z"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"eq", " ", "10", " ", "A_ij"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Axx", "=", 
   RowBox[{
    RowBox[{"Conjugate", "[", "dx\[Psi]", "]"}], ".", "dx\[Psi]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Axz", "=", 
   RowBox[{
    RowBox[{"Conjugate", "[", "dx\[Psi]", "]"}], ".", "dz\[Psi]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Azx", "=", 
   RowBox[{
    RowBox[{"Conjugate", "[", "dz\[Psi]", "]"}], ".", "dx\[Psi]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Azz", "=", 
    RowBox[{
     RowBox[{"Conjugate", "[", "dz\[Psi]", "]"}], ".", "dz\[Psi]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"eq", " ", "10", " ", "C_i"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Cx", "=", 
   RowBox[{
    RowBox[{"Conjugate", "[", "dx\[Psi]", "]"}], ".", "H", ".", "\[Psi]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Cz", "=", 
    RowBox[{
     RowBox[{"Conjugate", "[", "dz\[Psi]", "]"}], ".", "H", ".", "\[Psi]"}]}],
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"group", " ", "into", " ", "matrix", " ", "and", " ", "column"}], 
   " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
