 RowBox[{
  RowBox[{
   RowBox[{"Rz", "=", 
    RowBox[{"MatrixExp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], " ", "\[Theta]z", " ", 
      RowBox[{"Z", "/", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"parametrized", " ", "wavefunction"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Psi]", "=", 
   RowBox[{"Rz", ".", "Rx", ".", 
    RowBox[{"{", 
     RowBox[{"1", ",", "0"}], "}"}]}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", "Hamiltonian", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"H", "=", "Y"}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
